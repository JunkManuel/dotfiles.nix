# IMPORTANT: colors have to be defined in quotation marks.
# Otherwise the "#" and following would be interpreted as a comment.
[urgency_low]
    background = "{color8}b0"
    foreground = "{foreground}"
	frame_color = "{color9}"

[urgency_normal]
    background = "{background}b0"
    foreground = "{foreground}"
	frame_color = "{color12}"

[urgency_critical]
    background = "{color13}"
    foreground = "{foreground}"
    frame_color = "{color0}"
